        <param name="PDS_mock_arm2_current" value="stable"/>
        <param name="PDS_mock_arm3_current" value="stable"/>
        <param name="PDS_mock_arm4_current" value="stable"/>
        <param name="PDS_mock_arm5_current" value="stable"/>
        <param name="PDS_mock_arm6_current" value="stable"/>

