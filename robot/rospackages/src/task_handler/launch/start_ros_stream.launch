<launch>
    <arg name="_device_path" default="/dev/video0"/>
    <arg name="_image_width" default="480" />
    <arg name="_image_height" default="360" />
    <arg name="__name" default="video0Cam" />
    <node pkg="cv_camera" type="cv_camera_node" name="$(arg __name)" output="screen">
    <param name="_device_path" type="string" value="$(arg _device_path)"/>
    <param name="_image_width" type="int" value="$(arg _image_width)"/>
    <param name="_image_width" type="int" value="$(arg _image_height)"/>
    </node>
</launch>
